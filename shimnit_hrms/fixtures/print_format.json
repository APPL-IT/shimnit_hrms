oc.deductions[i] if i < doc.deductions|length else None %}\n        \n     \n        <tr class=\"\" style=\"text-align:center;\">\n            <td style=\"width:21%; text-align:left; \">{{ row1.salary_component }}</td>\n            <td style=\" width:7%;\">0.00</td>\n            <td style=\"width:7%; \">{{ row1.amount or 0.00 }}</td>\n            <td style=\"width:10%;\">0.00</td>\n            <td style=\"width:10%;\">{{ row1.amount or 0.00 }}</td>\n            <td style=\"width:35%; text-align: left;\">{{ row2.salary_component or \"\"}}</td>\n            <td style=\"width:10%; text-align: right;\"> {{ row2.amount or \"\"}}</td>\n        </tr>\n    {% endfor %}\n                <tr class=\"\" style=\"text-align:center; \">\n                    <td colspan=4 style=\"width 45%; text-align:left;\">Gross Earnings :</td>\n                    <td style=\"width:10%;\">{{ doc.gross_pay or \"\" }}</td>\n                    <td style=\"width:35%; text-align: left;\">Total Deductions :</td>\n                    <td style=\"width:10%; text-align: right;\"> {{ doc.total_deduction or \"\" }}</td>\n                </tr>\n                <tr class=\"\" style=\"text-align:center; \">\n                    <td colspan=7 style=\"width 45%; text-align:left;\">Net Salary : {{ doc.net_pay or \"\" }}</td>\n                </tr>\n                <tr class=\"\" style=\"text-align:center; \">\n                    <td colspan=7 style=\"width 45%; text-align:left;\">In words : {{ doc.total_in_words or \"\" }}</td>\n                </tr>\n                <tr class=\"\" style=\"text-align:center; font-weight;\">\n                    <td colspan=7 style=\"width 45%; text-align:left;\"></td>\n                </tr>\n                <tr class=\"\" style=\"text-align:center; font-weight: bold;\">\n                    <td colspan=7 style=\"width 45%; text-align:center;\">Income Tax Calculation for the financial Year 2025-2026</td>\n                </tr>\n                \n                <tr class=\"\"